<!DOCTYPE html>
<html lang="en">

<%- include("../partials/head.ejs") %>

    <body>
        <main>
            <form action="hc" method="post">
                <div class="container">
                    <label for="mascota" class="label">Nombre de la mascota</label>
                    <select name="mascota" id="mascota" class="input">
                        <option value="mascota">Michi</option>
                        <option value="mascota">Puchi</option>
                    </select>
                </div>
                <div class="container">
                    <label for="vete" class="label">Nombre del veterinario </label>
                    <select name="vete" id="vete" class="input">
                        <option value="pepe">Pepe</option>
                        <option value="claudio">Claudio</option>
                    </select>
                </div>
                <div class="container">
                    <label for="date" class="label">Fecha</label>
                    <input type="date" name="date" id="date">
                </div>
                <div class="container">
                    <label for="time" class="label">Hora</label>
                    <select name="time" id="time" class="input">
                        <option value="10">10</option>
                        <option value="11">11</option>
                        <option value="12">12</option>
                        <option value="13">13</option>
                    </select>
                </div>
            </form>
        </main>
        <%- include("../partials/footer.ejs") %>
    </body>

</html>